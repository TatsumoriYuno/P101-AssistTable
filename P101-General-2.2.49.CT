local player_movement_addr_memrecord = getAddressList().getMemoryRecordByID(5996)
local player_movement_addr = getAddressList().getMemoryRecordByID(5996).CurrentAddress --player movement address

function afk_spin() --performs afk spin even when window is unfocused

   writeInteger(player_movement_addr, 32) --32 spins to left, 64 spins to right
