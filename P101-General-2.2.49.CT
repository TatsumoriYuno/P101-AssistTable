              <AssemblerScript>//-- Speed Modifier for camera movement
globalalloc(speedModifier, 10)
speedModifier:
dd (float)10

{$lua}
KILL_TIMER = false

--local shouldAlternate = false --related to facing direction math

function listen_for_user_input()

--print(getAddressList().getMemoryRecordByDescription('potential camera yaw').CurrentAddress)

local yaw = readFloat(getAddressList().getMemoryRecordByDescription('Character camera yaw').CurrentAddress) --Horizontal (Yaw)
local pitch = readFloat(getAddressList().getMemoryRecordByDescription('potential camera pitch').CurrentAddress) --Vertical (Pitch)


--sine and cosine of rotation values
local siny = math.sin(yaw)  --Sine of Horizontal (yaw)
local cosy = math.cos(yaw)  --Cosine of Horizontal (yaw)
local sinp = math.sin(pitch) --Sine of Vertical (pitch)
local cosp = math.cos(pitch) --Cosine of Vertical (pitch)

local player_x = getAddressList().getMemoryRecordByDescription('Scene Camera X') --Cam X
local player_y = getAddressList().getMemoryRecordByDescription('Scene Camera Y') --Cam Y
local player_z = getAddressList().getMemoryRecordByDescription('Scene Camera Z') --Cam Z


--local increment_val = 20.0 -- previous value: 20.0
local increment_val = readFloat("speedModifier")

--move on x axis controls

--Forward
if(isKeyPressed(VK_NUMPAD8)) then
   --writeFloat(player_x.CurrentAddress, player_x.value + increment_val)  --original code
   writeFloat(player_y.CurrentAddress, player_y.value - (cosy * increment_val))
   writeFloat(player_x.CurrentAddress, player_x.value - (siny * increment_val))
   writeFloat(player_z.CurrentAddress, player_z.value - (sinp * increment_val))
end
--Backward
if(isKeyPressed(VK_NUMPAD2)) then
   --writeFloat(player_x.CurrentAddress, player_x.value - increment_val)
   writeFloat(player_y.CurrentAddress, player_y.value + (cosy * increment_val))
   writeFloat(player_x.CurrentAddress, player_x.value + (siny * increment_val))
   writeFloat(player_z.CurrentAddress, player_z.value + (sinp * increment_val))
end

--move on y axis controls

--Left
if(isKeyPressed(VK_NUMPAD4)) then
   --writeFloat(player_y.CurrentAddress, player_y.value + increment_val)
   writeFloat(player_y.CurrentAddress, player_y.value + (math.cos(yaw + math.rad(90)) * increment_val))
   writeFloat(player_x.CurrentAddress, player_x.value + (math.sin(yaw + math.rad(90)) * increment_val))
end
--Right
if(isKeyPressed(VK_NUMPAD6)) then
   --writeFloat(player_y.CurrentAddress, player_y.value - increment_val)
   writeFloat(player_y.CurrentAddress, player_y.value + (math.cos(yaw - math.rad(90)) * increment_val))
   writeFloat(player_x.CurrentAddress, player_x.value + (math.sin(yaw - math.rad(90)) * increment_val))
end

--Up
if(isKeyPressed(VK_SUBTRACT)) then
writeFloat(player_z.CurrentAddress, player_z.value + increment_val)
end
--Down
if(isKeyPressed(VK_ADD)) then
writeFloat(player_z.CurrentAddress, player_z.value - increment_val)
end

--Speed Modifier
if (isKeyPressed(VK_NUMPAD1)) then
   writeFloat("speedModifier", 500)
else -- if nothing is pressed, speed is normal
   writeFloat("speedModifier", 10)
end
end --end of listen_to_user_input function



function enableTimer()
    timer = createTimer(nil, true)
    timer.Interval = 1
    timer.OnTimer = function(timer)
    if KILL_TIMER == true then
        timer.destroy()
        --print("Timer destroyed")
    else
        listen_for_user_input()
        end
    end
end
[ENABLE]
enableTimer()
[DISABLE]

KILL_TIMER = true
