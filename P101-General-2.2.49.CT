function port_to_quest_foot()
-- script to teleport us from A to B (player to Quest dest)...
 local Quest_Dest_X = getAddressList().getMemoryRecordByDescription('Quest Dest X').value
local Quest_Dest_Y = getAddressList().getMemoryRecordByDescription('Quest Dest Y').value
local Quest_Dest_Z = getAddressList().getMemoryRecordByDescription('Quest Dest Z').value
 --print(string.format("%X", x_coord.CurrentAddress))
 -- print(Quest_Dest_X)
 local player_x_addr = getAddressList().getMemoryRecordByID(828).CurrentAddress --x address
local player_y_addr = getAddressList().getMemoryRecordByID(829).CurrentAddress --y address
local player_z_addr = getAddressList().getMemoryRecordByID(830).CurrentAddress --z address

writeFloat(player_x_addr, Quest_Dest_X)
writeFloat(player_y_addr, Quest_Dest_Y)
writeFloat(player_z_addr, Quest_Dest_Z)

end

function port_to_quest_shipMode()
 local Quest_Dest_X = getAddressList().getMemoryRecordByDescription('Quest Dest X').value
local Quest_Dest_Y = getAddressList().getMemoryRecordByDescription('Quest Dest Y').value
local Quest_Dest_Z = getAddressList().getMemoryRecordByDescription('Quest Dest Z').value
 local player_x_addr = getAddressList().getMemoryRecordByDescription('Ship X').CurrentAddress
local player_y_addr = getAddressList().getMemoryRecordByDescription('Ship Y').CurrentAddress
local player_z_addr = getAddressList().getMemoryRecordByDescription('Ship Z').CurrentAddress

writeFloat(player_x_addr, Quest_Dest_X)
writeFloat(player_y_addr, Quest_Dest_Y)
writeFloat(player_z_addr, Quest_Dest_Z)

end

function port_to_quest()
   port_to_quest_foot()
   if (getAddressList().getMemoryRecordByDescription('ship max speed ptr').value ~= nil) then
       port_to_quest_shipMode()
   end
end

[ENABLE]
port_to_quest()
[DISABLE]
port_to_quest()

